{
  "name": "svelte-pivottable",
  "version": "0.3.1",
  "description": "A Svelte-based pivot table with grouping",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jjagielka/svelte-pivottable.git"
  },
  "keywords": [
    "svelte",
    "sevltekit",
    "pivottable"
  ],
  "author": {
    "name": "Jakub Jagielka",
    "email": "jjagielka@gmail.com"
  },
  "license": "MIT",
  "files": ["dist"],
  "exports":{
    ".":{
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./PivotTable":{
      "types":"./dist/PivotTable.svelte.d.ts",
      "svelte":"./dist/PivotTable.svelte"
    },
    "./PivotTableUI":{
      "types":"./dist/PivotTableUI.svelte.d.ts",
      "svelte":"./dist/PivotTableUI.svelte"
    }
  },
  "bugs": {
    "url": "https://github.com/jjagielka/svelte-pivottable/issues"
  },
  "homepage": "https://github.com/jjagielka/svelte-pivottable#readme",
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.21.4",
    "@sveltejs/package": "^2.3.11",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@types/plotly.js": "^3.0.0",
    "@types/sortablejs": "^1.15.8",
    "svelte-check": "^4.2.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "peerDependencies": {
    "svelte": "^5.33.19"
  },
  "type": "module",
  "dependencies": {
    "sortablejs": "^1.15.6"
  }
}
